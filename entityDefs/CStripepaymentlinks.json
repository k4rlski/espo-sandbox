{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "stripelinkid": {
            "type": "varchar",
            "maxLength": 255,
            "options": [],
            "isCustom": true
        },
        "linkurl": {
            "type": "url",
            "isCustom": true
        },
        "linkstatus": {
            "type": "enum",
            "options": [
                "active",
                "inactive",
                "expired",
                "na"
            ],
            "style": {
                "active": null,
                "inactive": null,
                "expired": null,
                "na": null
            },
            "default": "na",
            "maxLength": 100,
            "isCustom": true
        },
        "amount": {
            "type": "currency",
            "isCustom": true
        },
        "totalrevenue": {
            "type": "currency",
            "isCustom": true
        },
        "totalclicks": {
            "type": "int",
            "isCustom": true
        },
        "totalconversions": {
            "type": "int",
            "isCustom": true
        },
        "conversionrate": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "lastused": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 1,
            "isCustom": true
        },
        "datecreated": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 1,
            "isCustom": true
        },
        "dateupdated": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 1,
            "isCustom": true
        },
        "dateexpires": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 1,
            "isCustom": true
        },
        "productname": {
            "type": "varchar",
            "maxLength": 255,
            "options": [],
            "isCustom": true
        },
        "metadata": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "isCustom": true
        },
        "allowquantity": {
            "notNull": true,
            "type": "bool",
            "isCustom": true
        },
        "collectshipping": {
            "notNull": true,
            "type": "bool",
            "isCustom": true
        },
        "collecttax": {
            "notNull": true,
            "type": "bool",
            "isCustom": true
        },
        "paymentmethods": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    }
}